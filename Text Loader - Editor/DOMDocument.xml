var page=1;
var pagech=1;

var totalnpage=int(root["numbertotal"]/4)+1;
if (int(this["numbertotal"]/4)==this["numbertotal"]/4) {
	totalnpage=totalnpage-1;
}

this.addEventListener(Event.ENTER_FRAME, givepages);
function givepages(e:Event):void {
	pagetxt.text=page;
	if (aw.leftpressed==true) {
		aw.leftpressed=false;
		if (page>1) {
			pagech=pagech-1;
		}
	}
	if (aw.rightpressed==true) {
		aw.rightpressed=false;
		if (page<totalnpage) {
			pagech=pagech+1;
		}
	}
	if (page!=pagech) {
		page=pagech;
		row1.out=true;
		row2.out=true;
		row3.out=true;
		row4.out=true;
	}
	if ((row1.waiting&&row2.waiting&&row3.waiting&&row4.waiting)==true) {
		for (var i=1; i<=4; i++) {
			if ((i+((page-1)*4))<=numbertotal){
			this["row"+i].number=i+((page-1)*4);
			} else {
				this["row"+i].number=0;
			}
		}
	}
	if ((row1.closee&&row2.closee&&row3.closee&&row4.closee)=="yes") {
		saveallbtn.removeEventListener(MouseEvent.CLICK, saveallfc);
		this.removeEventListener(Event.ENTER_FRAME, givepages);
		gotoAndStop(1, "Save");
	}
}

saveallbtn.addEventListener(MouseEvent.CLICK, saveallfc);
function saveallfc(e:MouseEvent):void{
	row1.closee=true;
	row2.closee=true;
	row3.closee=true;
	row4.closee=true;
}]]></script>
