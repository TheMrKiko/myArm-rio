                <script><![CDATA[var number;///o % é há quanto tempo não uso, comparado com hj
var waiting=true;
var seasons=["verão","ambos","inverno"];
var out=false;
var closee=false;
var lastdate_bk;
var lastdateold_bk;
this.visible=false;

addEventListener(Event.ENTER_FRAME, rowfunct);
function rowfunct(e:Event):void {
	seastxt.text=seasons[Number(seasonbox.text)+1];
	if (this.waiting==true) {
		if (this.number!=undefined&&this.number!=0) {
			trace("give "+this.number);
			this.visible=true;
			numberbox.text=String(this.number);
			itembox.text=root["item"+this.number];
			typebox.text=root["type"+this.number];
			subtypebox.text=root["subtype"+this.number];
			color0box.text=root["color"+this.number][0];
			color1box.text=root["color"+this.number][1];
			color2box.text=root["color"+this.number][2];
			color3box.text=root["color"+this.number][3];
			stylebox.text=root["style"+this.number];
			deltadaysbox.text=root["deltadays"+this.number];
			lastdatebox.daynumbs=Number(root["lastdate"+this.number][0]);
			lastdatebox.monthnumbs=Number(root["lastdate"+this.number][1]);
			lastdatebox.yearnumbs=Number(root["lastdate"+this.number][2]);
			lastdateoldbox.daynumbs=Number(root["lastdateold"+this.number][0]);
			lastdateoldbox.monthnumbs=Number(root["lastdateold"+this.number][1]);
			lastdateoldbox.yearnumbs=Number(root["lastdateold"+this.number][2]);
			seasonbox.text=root["season"+this.number];
			showbox.text=root["show"+this.number];
			imgbox.text=root["img"+this.number];
			this.waiting=false;
		} else {
			this.visible=false;
			numberbox.text="0";
			itembox.text="";
			typebox.text="";
			subtypebox.text="";
			color0box.text="0";
			color1box.text="0";
			color2box.text="0";
			color3box.text="0";
			stylebox.text="";
			deltadaysbox.text="";
			lastdatebox.daynumbs=1;
			lastdatebox.monthnumbs=0;
			lastdatebox.yearnumbs=2016;
			lastdate_bk=0;
			lastdateoldbox.daynumbs=1;
			lastdateoldbox.monthnumbs=0;
			lastdateoldbox.yearnumbs=2016;
			lastdateold_bk=0;
			seasonbox.text="0";
			showbox.text="1";
			imgbox.text="";
		}
	} else {
		this.visible=true;
	}
	if (this.out==true) {
		this.out=false;
		this.number=undefined;
		this.waiting=true;
	}
	if (root["greenv"+this.number]==true) {
		greenx.visible=true;
	} else {
		greenx.visible=false;
	}
	if (this.closee==true) {
		seasonbox.removeEventListener(MouseEvent.CLICK, seasoni);
		this.removeEventListener(MouseEvent.CLICK, rights);
		changebtn.removeEventListener(MouseEvent.CLICK, changething);
		removeEventListener(Event.ENTER_FRAME, rowfunct);
		savebtn.removeEventListener(MouseEvent.CLICK, savesingle);
		this.closee="yes";
	}
}

changebtn.addEventListener(MouseEvent.CLICK, changething);
function changething(e:MouseEvent):void {
	var lastdate_2=new Date(Number(lastdatebox.yearnumbs),Number(lastdatebox.monthnumbs),Number(lastdatebox.daynumbs),0,0,0,0);
	var lastdate_=lastdate_2.getTime()-(lastdate_2.getTimezoneOffset()*60000);
	var lastdateold_2=new Date(Number(lastdateoldbox.yearnumbs),Number(lastdateoldbox.monthnumbs),Number(lastdateoldbox.daynumbs),0,0,0,0);
	var lastdateold_=lastdateold_2.getTime()-(lastdateold_2.getTimezoneOffset()*60000);
	if (lastdate_!=lastdate_bk||lastdateold_!=lastdateold_bk) {
		lastdate_bk=lastdate_;
		lastdateold_bk=lastdateold_;
		var dateinterval=Math.abs(lastdate_-lastdateold_)/(1000*60*60*24);
		deltadaysbox.text=dateinterval;
	}
}

savebtn.addEventListener(MouseEvent.CLICK, savesingle);
function savesingle(e:MouseEvent):void {
	root["item"+this.number]=itembox.text;
	root["type"+this.number]=typebox.text;
	root["subtype"+this.number]=subtypebox.text;
	root["color"+this.number]=[];
	root["color"+this.number].push(color0box.text);
	root["color"+this.number].push(color1box.text);
	root["color"+this.number].push(color2box.text);
	root["color"+this.number].push(color3box.text);
	root["style"+this.number]=stylebox.text;
	root["season"+this.number]=seasonbox.text;
	root["lastdate"+this.number]=[];
	root["lastdate"+this.number].push(lastdatebox.daynumbs);
	root["lastdate"+this.number].push(lastdatebox.monthnumbs);
	root["lastdate"+this.number].push(lastdatebox.yearnumbs);
	root["lastdateold"+this.number]=[];
	root["lastdateold"+this.number].push(lastdateoldbox.daynumbs);
	root["lastdateold"+this.number].push(lastdateoldbox.monthnumbs);
	root["lastdateold"+this.number].push(lastdateoldbox.yearnumbs);
	root["deltadays"+this.number]=deltadaysbox.text;
	root["show"+this.number]=showbox.text;
	root["img"+this.number]=imgbox.text;
	root["greenv"+this.number]=true;
	trace("Item "+this.number);
	trace(root["item"+this.number]);
	trace(root["type"+this.number]);
	trace(root["subtype"+this.number]);
	trace(root["color"+this.number]);
	trace((root["style"+this.number])+"%");
	trace(root["season"+this.number]);
	trace(root["lastdate"+this.number]);
	trace(root["lastdateold"+this.number]);
	trace((root["deltadays"+this.number])+" dias");
	trace(root["show"+this.number]);
	trace(root["img"+this.number]);
}

seasonbox.addEventListener(MouseEvent.CLICK, seasoni);
function seasoni(e:MouseEvent):void {
	seasonbox.text=String(Number(seasonbox.text)+1);
	if (Number(seasonbox.text)>1) {
		seasonbox.text="-1";
	}
}

this.addEventListener(MouseEvent.CLICK, rights);
function rights(e:MouseEvent):void {
	if (e.target.name!="savebtn") {
		root["greenv"+this.number]=false;
	}
}]]></script>
